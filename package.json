{
  "name": "@dfuse/eosws-js",
  "version": "0.11.3",
  "description": "`eosws` JavaScript/TypeScript bindings (from the [dfuse API](https://dfuse.io/))",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist/*"
  ],
  "repository": "https://github.com/dfuse-io/eosws-js",
  "author": {
    "name": "dfuse Developers",
    "email": "dev@dfuse.io",
    "url": "https://dfuse.io"
  },
  "contributors": [
    {
      "name": "Denis Carriere",
      "email": "denis@eosnation.io",
      "url": "https://eosnation.io"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepare": "tsc",
    "lint": "tslint **/*.ts",
    "lint:specific": "tslint",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "_comment": "Use --no-verify to bypass",
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.ts": [
        "yarn lint:specific"
      ]
    },
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.9",
    "@types/ws": "^6.0.1",
    "dotenv": "^6.1.0",
    "husky": "^1.1.2",
    "isomorphic-ws": "^4.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3",
    "ws": "^6.1.0"
  },
  "dependencies": {
    "debug": "^4.1.0"
  }
}
